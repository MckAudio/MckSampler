{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/deps/JUCE/modules/**",
                "${workspaceFolder}/deps/concurrentqueue/**",
                "${workspaceFolder}/deps/MckJuce/Source/**",
                "${workspaceFolder}/deps/MckHelper/include/**",
                "${workspaceFolder}/deps/DaisySP/**"
            ],
            "defines": ["RPI_MODE"],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "gcc-x64"
